             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd"
             version="2.2">

  <persistence-unit name="clinique">


    <class>model.Client</class>
    <class>model.Medecin</class>
    <class>model.Rdv</class>
    <properties>
      <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/cabinetims"/>
